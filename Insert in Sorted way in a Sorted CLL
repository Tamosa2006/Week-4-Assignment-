class Solution:
    def sortedInsert(self, head, x):
        new_node = Node(x)

        if head is None:
            return new_node

        if x < head.data:
            new_node.next = head
            head.prev = new_node
            return new_node

        curr = head
        while curr.next and curr.next.data < x:
            curr = curr.next

        new_node.next = curr.next
        new_node.prev = curr

        if curr.next:
            curr.next.prev = new_node
        curr.next = new_node

        return head
